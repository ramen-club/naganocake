<h2>配送先編集</h2>

<%= form_for(@deliver) do |f| %>

  <div class="field">
    <%= f.label "郵便番号（ハイフンなし）" %>
    <%= f.text_field :postal_code, class: "postal_code" %>
  </div>

  <div class="field">
    <%= f.label "住所" %>
    <%= f.text_field :street_address, class: "street_address" %>
  </div>

   <div class="field">
    <%= f.label "宛名" %>
    <%= f.text_field :name, class: "family_name" %>
  </div>


  <div class="actions">
    <%= f.submit "編集内容を保存する", class: "btn btn-primary"%>
  </div>

<% end %>
