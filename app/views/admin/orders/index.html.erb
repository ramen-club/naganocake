<h3>注文一覧画面</h3>
<br>

<table>
   <thead>
      <tr>
        <th>購入日時</th>
        <th>購入者</th>
        <th>注文個数</th>
        <th>注文ステータス</th>
      </tr>
  </thead>

  <tbody>
  <%= @orders.each do |order| %>
    <tr>
      <td><%= order.created_at %></td>
      <td><%= order.name %></td>
      <td><%= <%#注文個数？#%> %></td>
      <td><%= order.order_status %></td>
    </tr>
  <% end %>
  </tbody>
  