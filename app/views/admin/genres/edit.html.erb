<h3>ジャンル編集画面</h3>
<br>
<%# form_with ヘルパーを使いたかっただけです。 %>
<%= form_with(model: @genre, url: admin_genre_path) do |f| %>
  <%= "ジャンル" %>
  <%= f.text_field :name %>
   <%= f.radio_button :is_active, :on %>
   <%= f.label :is_active, "有効", {value: :on} %>
   <%= f.radio_button :is_active, :off %>
   <%= f.label :is_active, "無効", {value: :off} %>
   <%= f.submit "変更を編集する"%>
<% end %>