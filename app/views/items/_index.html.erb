<!-- 商品一覧の部分テンプレート -->
<div class="main clearfix">
  <% items.each do |item| %>
    <% if item.genre.is_active == "有効" %>

      <div class = "AA">
        <div class = "imagWrap-f">
          <%= link_to item_path(item) do %>
            <%= attachment_image_tag item, :image, :fill, 60, 60, fallback: "no_image.jpg", size:'150x100' %>
          <% end %>
        </div>
      <p><%= link_to item.name, item_path(item) %></p>
      <p><%= number_with_delimiter(tax_included_price(item.price)) %></p>
      </div>

    <% end %>
  <% end %>
</div>